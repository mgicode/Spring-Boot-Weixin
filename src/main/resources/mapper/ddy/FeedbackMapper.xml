<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.xujianhua.demo.ddy.dao.FeedbackMapper">

	

	<select id="getById" resultType="com.github.xujianhua.demo.ddy.entity.FeedbackEntity">
		select * from ddy_feedback where id = #{id}
	</select>
	
    <select id="findList" resultType="com.github.xujianhua.demo.ddy.entity.FeedbackEntity">
        select * from ddy_feedback order by id desc
    </select>
	
</mapper>